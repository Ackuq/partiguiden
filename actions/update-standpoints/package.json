{
  "name": "@partiguiden/update-standpoints",
  "version": "1.0.0",
  "description": "Github Action for updating standpoints and creating pull requests",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "pnpm dlx rimraf dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "bundle:watch": "pnpm run bundle --watch",
    "clean": "rm -rf .turbo node_modules",
    "lint:eslint": "eslint",
    "lint:tsc": "tsc"
  },
  "dependencies": {
    "@actions/core": "catalog:",
    "@actions/exec": "catalog:",
    "@actions/github": "catalog:"
  },
  "devDependencies": {
    "@partiguiden/eslint-config": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "eslint": "catalog:",
    "rollup": "catalog:"
  }
}
