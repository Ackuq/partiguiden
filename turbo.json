{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": [
        "NEXT_PUBLIC_*",
        "GA4_*",
        "SENTRY_*",
        "ANALYZE",
        "VERCEL",
        "ENABLE_EXPERIMENTAL_COREPACK"
      ],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint:eslint": {
      "dependsOn": [],
      "outputs": []
    },
    "lint:tsc": {
      "dependsOn": [],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": ["NODE_ENV"],
  "globalPassThroughEnv": ["COREPACK_HOME"]
}
